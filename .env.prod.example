# LegalOSS Production Environment
# Copy to .env.local and fill in your values

# Your Case.dev API key (required)
CASE_API_KEY=your_case_api_key_here

# Generate a secure secret: openssl rand -base64 32
AUTH_SECRET=your_secure_secret_here
NEXTAUTH_SECRET=your_secure_secret_here

# Your production domain
NEXTAUTH_URL=https://your-domain.com
AUTH_TRUST_HOST=true

# Case.dev API (default)
CASE_API_URL=https://api.case.dev

# Production mode - no dev features
IS_DEV=false
